body
	background: #e5e5e5
	color black
.miembro_img
	width: 150px
	height: auto

a 
	color #52b6ec
	transition: 300ms;
h1,h2,h3,h4,h5,h6
	color #777
	font-size: 40px
	
#header
	height: 100px
	position: fixed
	width 100%
	z-index: 1111
	
#main-slider
	background url(../img/1.jpg)
	background-attachment: fixed
	background-position: 50% 50%;
	background-repeat: no-repeat;
	background-size: cover
	color white
	padding: 150px 0
	.item
		text-align: center
		background rgba(0, 0, 0, .75)
	.right, .left
		background-color: rgba(0, 0, 0, 0.3);
		border-radius: 4px;
		color: #fff;
		display: inline-block;
		font-size: 24px;
		height: 50px;
		line-height: 50px;
		margin-top: -50px;
		position: absolute;
		text-align: center;
		top: 50%;
		width: 50px;
		z-index: 5;
		
#team-scroller
	padding .5em 6em 
	.right, .left
		background-color: rgba(0, 0, 0, 0.0);
		border-radius: 4px;
		color: #fff;
		display: inline-block;
		font-size: 24px;
		height: 50px;
		line-height: 50px;
		margin-top: -50px;
		position: absolute;
		text-align: center;
		top: 50%;
		width: 50px;
		z-index: 5;
	.item.active h1
		animation: scaleUp 400ms;
		
	
.caja
	background white
	border-bottom: 1px solid #e9e9e9
	padding: 50px 30px
	position: relative
	z-index: 99
.primerCaja 
	border-radius: 5px 5px 
	margin-top: -40px
	
#contacto .box
	background #222
	color #999
	
#contacto .box h1,#contacto .box h2,#contacto .box h3,#contacto .box a
	color #999
	
#contacto .box a:hover
	color #52b6ec
	
#contacto .box input[type="text"],#contacto .box input[type="email"],#contacto .box textarea
	background-color #111
	border 0
	transition: 300ms;
	
.box
	background #fff
	border-bottom 1px solid #e9e9e9
	padding 30px
	
.box h2
	border-radius: 4px;
	display: inline-block;
	font-size: 48px;
	margin-bottom: 30px;
	padding: 10px 30px;
	position: relative;

.box.last {
	border-radius: 0 0 5px 5px;
}

#paquetes .box
	background-color #f9f9f9
#paquetes .paquete1
	background: #DDD
	border-bottom: 1px solid #e1e1e1;
	border-radius .5em
	list-style: none;
	margin: 0 0 20px;
	padding: 0;
	text-align: center;
	
#paquetes .paquete1 li 
	font-size 18px
	font-weight: 600
	padding 10px 0
	
#paquetes .paquete1 li.precio_paquete
	color #666
	font-size 48px
	padding 15px 0
#paquetes .paquete1 li.action_paquete
	border-bottom 0
	margin-bottom 20px
	margin-top 20px

#servicios .box
	padding-buttom 0
#servicios .row > div
	margin-bottom 50px
	
.centro 
	text-align: center	

#team-scroller .area
	margin-top 10px
	display block
	
.miembro 
	//margin 0 20px
	text-align: center


.icon-google-plus,.icon-facebook2,.icon-twitter,.icon-github
	font-size 3em